RewriteEngine on
RewriteCond %{HTTP_HOST} ^(www.)?catchme-v2.krishanmadan.website$
RewriteCond %{REQUEST_URI} !^/public/
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ /public/$1
RewriteCond %{HTTP_HOST} ^(www.)?catchme-v2.krishanmadan.website$
RewriteRule ^(/)?$ public/ [L]